<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Text?>

<VBox fx:id="vbMain" alignment="CENTER" prefHeight="566.0" prefWidth="553.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="com.example.demo.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" />
    </padding>
   <children>
      <AnchorPane fx:id="apMain" prefHeight="453.0" prefWidth="513.0">
         <children>
            <StackPane layoutX="34.0" layoutY="39.0" onMouseClicked="#onVertexClicked" onMouseDragged="#onMouseDragged" onMousePressed="#onMousePressed" prefHeight="96.0" prefWidth="91.0">
               <children>
                  <Circle fill="DODGERBLUE" radius="41.0" stroke="BLACK" strokeType="INSIDE" />
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="A" />
               </children>
            </StackPane>
         </children>
      </AnchorPane>
      <GridPane>
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button mnemonicParsing="false" onAction="#onSaveClicked" prefHeight="25.0" prefWidth="149.0" text="Save" />
            <Button mnemonicParsing="false" onAction="#onAddClicked" prefHeight="25.0" prefWidth="161.0" text="Add" GridPane.columnIndex="1" />
            <Button mnemonicParsing="false" onAction="#onRemoveClicked" prefHeight="25.0" prefWidth="151.0" text="Remove" GridPane.columnIndex="2" />
         </children>
      </GridPane>
   </children>
</VBox>
